<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:MyWpf.Resources"
                    xmlns:convert="clr-namespace:MyWpf.Converters"
                    xmlns:common="clr-namespace:MyWpf.Common">
    
    <Style x:Key="WindowStyle" TargetType="Window">
        <Setter Property="WindowStyle" Value="None"></Setter>
        <Setter Property="AllowsTransparency" Value="True"></Setter>
    </Style>

    <SolidColorBrush Color="#FFF0EBEB" x:Key="DialogBackground"></SolidColorBrush>
    
    <convert:TextConverter x:Key="textConvert"></convert:TextConverter>
    <convert:ImageToStringConverter x:Key="imaToStr"></convert:ImageToStringConverter>
    <convert:MultiParamConverter x:Key="multiParam"></convert:MultiParamConverter>
    
    <common:ThemeConfig x:Key="theme"></common:ThemeConfig>
</ResourceDictionary>